4}}],"sources":{"arithmetic.js":"'use strict';\r\n// Operators\r\n// + addition\r\n// - minus\r\n// * multiplication\r\n// / division\r\n// % modulus\r\n// ++ increment\r\n// -- decrement\r\n// ** exponential\r\n\r\n// Comparison Operators\r\n// == equals\r\n// === equals to value and type\r\n// != not equal\r\n// !== not equal to type and value\r\n// < less than\r\n// > greater than\r\n// < == less than or equals to \r\n// > == greater than or equals to\r\n\r\nvar y;\r\nvar x;\r\n\r\n//\r\n\r\n\r\nconst arithmetic =(operators)=>{\r\n    if (operators == \"addition\")\r\n    {\r\n        return x = 2+2;\r\n    }\r\n    else if (operators == \"subtraction\")\r\n    {\r\n        return x = 2-2;\r\n    }\r\n    else if (operators == \"multiplication\")\r\n    {\r\n        return x = 2*2;\r\n    }\r\n    else if (operators == \"division\")\r\n    {\r\n        return x = 2/2;\r\n    }\r\n    else if (operators == \"modulus\")\r\n    {\r\n        return x = 6%2;\r\n    }\r\n    else if (operators == \"increment\")\r\n    {\r\n        x = 6;\r\n        return x++;\r\n    }\r\n    else if (operators == \"decrement\")\r\n    {\r\n        x = 6;\r\n        return x--;\r\n    }\r\n    else if (operators == \"exponential\")\r\n    {\r\n        return x = 6**2;\r\n    }\r\n    else\r\n    {\r\n        return x = \"no value\"\r\n    }\r\n};\r\n\r\nconst comparison = (operators,a,b)=>{\r\n    if (operators == \"equals\")\r\n    {\r\n        return y = Boolean(a==b);\r\n    }\r\n    else if (operators == \"equals_typeAndvalue\")\r\n    {\r\n        return y =   Boolean(a===b);\r\n    }\r\n    else if (operators == \"not_equal\")\r\n    {\r\n        return y =   Boolean(a!=b);\r\n    }\r\n    else if (operators == \"not_equals_typeAndvalue\")\r\n    {\r\n        return y =   Boolean(a!==b);\r\n    }\r\n    else if (operators == \"greater\")\r\n    {\r\n        return y =   Boolean(a>b);\r\n    }\r\n    else if (operators == \"lesser\")\r\n    {\r\n        return y =   Boolean(a<b);\r\n    }\r\n    else if (operators == \"greaterthenorequal\")\r\n    {\r\n        return y =  Boolean(a>=b);\r\n    }\r\n    else if (operators == \"lessthenorequal\")\r\n    {\r\n        return y =   Boolean(a<=b);\r\n    }\r\n    else\r\n    {\r\n        return y = \"no value\"\r\n    }\r\n};\r\n\r\nvar i = arithmetic(\"multiplication\");\r\nvar j = comparison(\"lesser\",2,3);\r\n\r\nfunction calc() {\r\n   \r\n    document.getElementById(\"demo5\").innerHTML =[\"arithmetic: \",  i, \" comparison: \", j];\r\n}\r\n\r\ncalc();\r\n"},"lineCount":null}},"error":null,"hash":"eec8f251e530900f099e60775bf61f0d","cacheData":{"env":{}}}